	<div class="filter-subcontainer">	
		<p class="filter-p">Reason for Visit</p>
		<div id="filter-reason" class="collapse filter-spec">
		  <div >
			  <label class="control-label" for="filter-reason-input">Enter Reason:
			  <input type="text" class="form-control col-sm-12" id="filter-reason-input" ></label>
		  </div>
		</div>
	</div>
	
	<div class="filter-subcontainer">	
		<p class="filter-p">Mode of Visit</p>
		<div id="filter-mode-of-visit" class="collapse filter-spec">
		  <label class="control-label" for="filter-mode-of-visit-select">Select Mode of Visit</label>
		  <select id="filter-mode-of-visit-select" class="custom-select full-select" >
			  <option value="0" selected>--Select mode of visit--</option>
			  <option value="1">SCOAN Invited</option>
			  <option value="2">SCOAN by Gate</option>
			  <option value="3">Special Guest</option>
		  </select>
		</div>
	</div>
	
	<div class="filter-subcontainer">	
		<p class="filter-p">Reference No</p>
		<div id="filter-reference-no" class="collapse filter-spec">
		  <label class="control-label" for="filter-reference-no-select">Select Mode of Visit</label>
		  <select id="filter-reference-no-select" class="custom-select full-select" >
			  <option value="0" selected>--Select Reference No-</option>
			  <option value="1">1</option>
			  <option value="2">2</option>
			  <option value="3">3</option>
			  <option value="4">4</option>
			  <option value="5">5</option>
		  </select>
		</div>
	</div>
	
	<div class="filter-subcontainer">	
		<p class="filter-p">Dining</p>
		<div id="filter-dining" class="collapse filter-spec">
		  <label class="control-label" for="filter-dining-select">Select Dining</label>
		  <select id="filter-dining-select" class="custom-select full-select" >
			  <option value="0" selected>--Select Dining--</option>
			  <option value="1">Newland 1 Dining</option>
			  <option value="2">Newland 2 Dining</option>
			  <option value="3">Up Dining</option>
			  <option value="4">Botswana Dining</option>
			  <option value="5">Private Dining</option>	
		  </select>
		</div>
	</div>
	
